<theme>
   <formatVersion>7</formatVersion>
   <variables ifSubset="colorset:blue">
      <menuSelectedColor>ffffff</menuSelectedColor>
      <menuSelectorColor>FF9E00cc</menuSelectorColor>
      <menuSelectorColorEnd>FF9E00</menuSelectorColorEnd>
   </variables>
   <variables ifSubset="colorset:black">
      <menuSelectedColor>FF9E00</menuSelectedColor>
   </variables>
   <variables>
      <sistema.lineainferior>FF9E00</sistema.lineainferior>
      <splash.progressbarActive>FF9E00</splash.progressbarActive>
      <gamelistSelectorColor>FF9E00</gamelistSelectorColor>
      <backgroundgridSelect>FF9E00</backgroundgridSelect>
      <gamelist.starFill>FF9E00</gamelist.starFill>
      <grid.starFill>FF9E00</grid.starFill>
   </variables>
</theme>